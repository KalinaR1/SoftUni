first_number = int(input())
second_number = int(input())
third_number = int(input())


def smallest():
    res = None
    if first_number < second_number and first_number < third_number:
        res = first_number
    elif second_number < first_number and second_number < third_number:
        res = second_number
    else:
        res = third_number
    return res


print(smallest())
