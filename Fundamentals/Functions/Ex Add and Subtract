first_number = int(input())
second_number = int(input())
third_number = int(input())


def sum_numbers():
    result_1 = first_number + second_number
    return result_1


def subtract():
    result_2 = sum_numbers() - third_number
    return result_2


def add_and_subtract():
    return subtract()


print(add_and_subtract())
